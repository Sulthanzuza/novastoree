<head><link rel="stylesheet" href="/products.css">
<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha3/dist/css/bootstrap.min.css" rel="stylesheet">
<link rel="stylesheet" href="https://cdn.datatables.net/1.13.4/css/jquery.dataTables.min.css">
<title>{{title}}</title>
</head>

{{>dashp}}
 <div class="products-containers">
    <div class="layout-wrappers" style="height:105%">
      <main class="main-contents">
        <header class="headers">
          <nav class="breadcrumb" aria-label="Breadcrumb">
            <span class="breadcrumb-text">Admin</span>
            <img src="https://cdn.builder.io/api/v1/image/assets/TEMP/347fc193d0088dca0695d99147b3700ee95d8efb9ecfa3c2f8ce353113eebee3?placeholderIfAbsent=true&apiKey=d7cebeeadab64378ab4fc611373387df" alt="" />
            <span class="breadcrumb-text breadcrumb-active">Settings</span>
          </nav>
          <a class="logout-button" aria-label="Logout"  href="/admin/logout">
            <img src="https://cdn.builder.io/api/v1/image/assets/TEMP/434f453e4cc985013ba5977dc6c3ec92bd5cb52d93cf0e330a3970e94bfbd360?placeholderIfAbsent=true&apiKey=d7cebeeadab64378ab4fc611373387df" alt="" />
          </a>
        </header>

        <section class="products-grid">
          
      <div class="container" >
  <div class="text-center mb-4 ">
    <h1 class="logo">Change Password</h1>
  </div>

  <div class="text-center login-box mx-auto" style="text-align: center;" >
    <form id="change-password-form">
      <div class="mb-3" style="width:60%;  ">
  <label for="current-password" class="form-label">Enter Current Password</label>
  <input type="password" class="form-control" style="margin-left: 35%;" id="current-password" name="password" placeholder="Enter your current password" required>
</div>
<div class="mb-3" style="width:60%;  ">
  <label for="new-password" class="form-label">Enter New Password</label>
  <input type="password" class="form-control" id="new-password" style="margin-left: 35%;" name="password" placeholder="Enter your new password" required>
</div>
<div class="mb-3" style="width:60%;  ">
  <label for="confirm-password" class="form-label">Confirm New Password</label>
  <input type="password" class="form-control" id="confirm-password" style="margin-left: 35%;" name="password" placeholder="Confirm your new password" required>
</div>


      <div class="d-grid" style="width:60%;   ">
        <button type="submit" style="margin-left: 55%;" class="btn btn-dark">Change Password</button>
      </div>
    </form>

    <div id="message" class="mt-3 text-center"></div>
  </div>
</div>


        </section>
      </main>
    </div>
<script src="/js/settings.js"></script>